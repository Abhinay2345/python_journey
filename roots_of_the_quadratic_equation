import math

a=float(input("a:"))
b=float(input("b:"))
c=float(input("c:"))

delta=math.pow(b,2)-(4*a*c)
if delta>0:
  x1=((-b)+math.sqrt(delta)//(2 * a))
  x2=((-b)-math.sqrt(delta)//(2 * a))
  print("there are 2 roots %f and %f"%(x1,x2))
elif delta==0:
  x=(-b)//2 * a
  print("there is one root;",x)
else:
  print("no roots")
